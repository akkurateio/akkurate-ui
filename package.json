{
  "name": "akkurate-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate-icons": "ts-node ./tools/cli/icons-generator/cli.ts",
    "publish": "yarn build && lerna publish --registry=https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^18.7.9",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "eslint": "8.19.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react": "1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.5.1",
    "find-up": "^6.3.0",
    "husky": "^7.0.4",
    "lerna": "^5.4.3",
    "lint-staged": "11.2.6",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.2",
    "turbo": "latest",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.8",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "framer-motion": "^7.2.0"
  },
  "packageManager": "yarn@1.22.18",
  "main": "index.js",
  "repository": "git@github.com:akkurateio/akkurate-ui.git",
  "author": "Benjamin <benjamin.francois13@gmail.com>",
  "license": "MIT"
}
