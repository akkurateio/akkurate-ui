{
  "name": "@akkurateio/forms",
  "version": "0.0.1",
  "description": "Library for Akkurate UI forms components",
  "keywords": [
    "icons"
  ],
  "author": "Subvitamine",
  "license": "proprietary",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akkurateio/akkurate-ui.git",
    "directory": "packages/forms"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.0.1",
    "@types/react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": ">=2.0.0",
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "framer-motion": ">=7.0.0",
    "react-colorful": ">=5.0.0",
    "styled-components": "5.3.5",
    "@akkurateio/icons": "^0.0.10"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "yarn build -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "JSX=1 tsup src/index.ts"
  }
